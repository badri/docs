  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/shapeblock.png",
    "href": "https://www.shapeblock.com"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#342149",
    "light": "#07C983",
